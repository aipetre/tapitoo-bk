                        </ul>
                    </li>

                </ul>

                {#<form class="navbar-search pull-right">#}
                    {#<input type="text" class="search-query" data-i18n="[placeholder]topRightBar.search">#}
                {#</form>#}

            </div>
            <!--/.nav-collapse -->

        </div>
        <!-- /container -->

    </div>
    <!-- /navbar-inner -->

</div>
