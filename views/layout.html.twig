                        {#</ul>#}

                    {#</li>#}

                    <li class="dropdown">

                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                            <i class="icon-user"></i>{% if globals.type != "superadmin" %}Restaurant{% else %}Super Admin{% endif %}<b class="caret"></b>
                        </a>

                        <ul class="dropdown-menu">
                            {#<li><a href="javascript:;" data-i18n="topRightBar.my_profile">My Profile</a></li>#}
