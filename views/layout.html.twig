        <link href="/css/custom.css" rel="stylesheet"/>
        <link href="/css/Styles/msgBoxLight.css" rel="stylesheet" type="text/css"/>
    {% endblock %}

    {% block customstylesheets %}
        <!-- Custom stylesheet-->
    {% endblock %}

    {% block javascripts %}
        <script src="http://code.jquery.com/jquery-1.9.1.js"></script>
        <script src="http://code.jquery.com/ui/1.10.3/jquery-ui.js"></script>
        <script src="/js/plugins/i18next/i18next-1.7.1.js"></script>
        <script src="/js/plugins/i18next/i18next-1.7.1.min.js"></script>
        <script src="/js/translation.js"></script>
        <script src="/socket.io/socket.io.js"></script>
        <script type="text/javascript">
            // Set global restaurant and location IDs. Not very ideal :(
            document.restaurantId = "{{ globals.restaurantId }}";
            document.locationId = "{{ globals.locationId  }}";
        </script>
        <script src="/js/comet.js"></script>
    {% endblock %}

    <!-- Le HTML5 shim, for IE6-8 support of HTML5 elements -->
    <!--[if lt IE 9]>
    <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->

    {% block customjavascript %}
        <!-- Custom javascript-->
    {% endblock %}
</head>

<body>

