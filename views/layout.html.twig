    <div class="navbar-inner">

        <div class="container">

            <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
                <i class="icon-cog"></i>
            </a>

            <a class="brand" href="/">
                {% if globals.displayName is not empty %}
                    {{ globals.displayName }} {% if globals.locationDisplayName is not empty %}{{ globals.locationDisplayName }}{%  endif %}
